-- Initialize Library
local Library = loadstring(game:HttpGet("https://example.com/library"))()  -- Replace with actual library URL
local Window = Library.CreateLib("TITLE", "DarkTheme")

-- Create Tools Tab
local Tab = Window:NewTab("Tools")

-- Create HomeFrame in the Tools Tab
local HomeFrame = Instance.new("Frame")
HomeFrame.Parent = Tab
HomeFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
HomeFrame.BorderColor3 = Color3.fromRGB(255, 0, 0)
HomeFrame.BorderSizePixel = 3
HomeFrame.Size = UDim2.new(0, 400, 0, 247)
HomeFrame.Visible = true

-- Create FlyButton in the Tools Tab
local FlyButton = Instance.new("TextButton")
FlyButton.Parent = HomeFrame
FlyButton.BackgroundColor3 = Color3.fromRGB(0, 0, 255) -- Blue
FlyButton.Position = UDim2.new(0, 10, 0, 10)
FlyButton.Size = UDim2.new(0, 100, 0, 40)
FlyButton.Text = "Fly"
FlyButton.TextColor3 = Color3.fromRGB(255, 255, 255)

